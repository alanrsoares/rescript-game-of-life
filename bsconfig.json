{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "reason-game-of-life",
  "reason": { "react-jsx": 3 },
  "bsc-flags": ["-bs-super-errors"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true,
      "suffix": ".bs.js"
    }
  ],
  "external-stdlib": "@rescript/std",
  "namespace": true,
  "bs-dependencies": [
    "reason-react",
    "bs-css",
    "bs-css-emotion",
    "@glennsl/bs-jest"
  ],
  "ppx-flags": ["@davesnx/styled-ppx/ppx"],
  "refmt": 3,
  "warnings": {
    "error": "+101"
  }
}
